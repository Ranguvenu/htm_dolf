{{#response}}
    {{#records}}
    <div class= "table-responsive">
        <table class="table">
            <thead class="thead-light">
                <tr>

                  <th scope="col">{{# str}} name, local_userapproval{{/ str}}</th>
                  <th scope="col">{{# str}} email, local_userapproval{{/ str}}</th>
                  <th scope="col">{{# str}} mobile, local_userapproval{{/ str}}</th>
                  <th scope="col">{{# str}} idnumber, local_userapproval{{/ str}}</th>
                  <th scope="col">{{# str}} sector, local_userapproval{{/ str}}</th>
                  <th scope="col">{{# str}} organization, local_userapproval{{/ str}}</th>
                  <th scope="col">{{# str}} status, local_userapproval{{/ str}}</th>
                  <th scope="col">{{# str}} examenrolledusers, local_userapproval{{/ str}}</th>
                  <th scope="col">{{# str}} tpenrolledusers, local_userapproval{{/ str}}</th>
                  <th scope="col">{{# str}} eventenrolledusers, local_userapproval{{/ str}}</th>
                  <th class="text-center">{{# str}} actions, local_userapproval{{/ str}}</th>
                </tr>
            </thead>
          <tbody>
          {{# hascourses}}
              <tr>
                  <td>{{{displayfullname}}}</a></td>
                  <td>{{email}}</td>
                  <td>{{mobile}}</td>                  
                  <td>{{id_number}}</td>                  
                  <td>{{sector}}</td>                  
                  <td>{{organization}}</td>                  
                  <td>{{status}}</td>
                  <td class="text-center">
                   <a href="#" data-action="itemenrolledlist" data-id={{userid}} data-name='{{fullname}}' data-type='exams'>
                        {{examenrolledusers}}
                   </a>
                  </td>
                  <td class="text-center"> <a href="#" data-action="itemenrolledlist" data-id={{userid}} data-name='{{fullname}}' data-type='programs'>
                        {{tpenrolledusers}}
                   </a> </td>
                  <td class="text-center"><a href="#" data-action="itemenrolledlist" data-id={{userid}} data-name='{{fullname}}' data-type='events'>
                        {{eventenrolledusers}}
                   </a> </td>
                  <td>
                      <div class="d-flex action_btns justify-content-center">
                        <a href="#"  data-action="viewuserregistration" data-id={{id}} data-name='{{fullname}}' data-toggle="tooltip" title={{#str}}view,local_userapproval{{/str}}><span class="bg-info text-white"><i class="fa fa-search-plus"></i></span></a>
                          {{# isorgofficial}}
                            <a href="#"  data-action="createuser" data-id={{id}} data-segment={{segmentid}} data-jobfamily={{jobfamilyid}} data-jobrole={{jobroleid}} ><span class="bg-primary text-white" data-toggle="tooltip" title="{{edit}}"><i class="fa fa-pencil fa-fw"></i></span></a>
                          {{/ isorgofficial}}
                          {{# manageuser}}
                             {{#approvedstatus}}
                             {{^deletedstatus}}
                              <a href="#"  data-action="approveuser" data-id={{id}} data-name='{{fullname}}' data-toggle="tooltip" title="{{approve}}"><span class="bg-primary text-white"><i class="fa fa-check"></i></span></a>
                              <a href="#" data-action="rejectuser" data-id={{id}} data-name='{{fullname}}' data-toggle="tooltip" title="{{reject}}"><span class="bg-warning text-white"><i class="fa fa-times"></i></span></a>
                              {{/deletedstatus}}
                              {{/approvedstatus}}
                              {{^deletedstatus}}
                              <a href="#" data-action="deleteuser" data-id={{id}} data-name='{{fullname}}' data-toggle="tooltip" title="{{delete}}"><span class="bg-danger text-white"><i class="fa fa-trash-o" aria-hidden="true"></i></span></a>
                              {{/deletedstatus}}
                              {{# rejectedactiondisplay}}
                                {{^ deletedstatus}}
                              <a href="#"  data-action="createuser" data-id={{id}} data-segment={{segmentid}} data-jobfamily={{jobfamilyid}} data-jobrole={{jobroleid}} ><span class="bg-info text-white" data-toggle="tooltip" title="{{edit}}"><i class="fa fa-pencil fa-fw"></i></span></a>
                              <a href="#" data-action="sendemailtouser" data-id="{{userid}}" data-email="{{email}}"  data-name="{{fullname}}"><span class="bg-info text-white" data-toggle="tooltip" title="{{sendemail}}"><i style="margin: 0.5rem;" class="icon fa fa-envelope-square"></i></span></a>
                                {{/ deletedstatus}}
                              {{/ rejectedactiondisplay}}
                          {{/ manageuser}}
                      </div>
                  </td>      
              </tr>
           {{/ hascourses}} 
          </tbody>
        </table>
        </div>
    {{/ records}}
{{/ response}}
