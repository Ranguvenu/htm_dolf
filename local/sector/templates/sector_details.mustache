{{#response}}
{{#records}}
{{#sector}}
<div class="sector_content mb-3">
    <div class="sector_header d-flex align-items-center justify-content-between py-3">
        <h5> {{title}}</h5>
        <div class="d-flex action_btns">
            <a class="theme_text_color1 mr-3" data-action="createsegment" data-sectorname="{{title}}" data-sectorid={{sectorid}}>{{# str}}add_seg, local_sector{{/ str}}</a>
            <a class="theme_text_color1" data-action="createsector" data-id={{sectorid}} title="{{#str}} edit, local_sector {{/str}}">
            <span class="bg-info text-white"><i class="fa fa-pencil fa-fw"></i></span></a>
            <a class="theme_text_color1 " data-action="deletesector" data-id={{sectorid}}  title="{{#str}}delete,local_sector{{/str}}">
            <span class="bg-danger text-white"><i class="fa fa-trash-o" aria-hidden="true"></i></span></a>
   

        </div>
    </div>
    {{#segment}}
    <div class="sector_detail_content">
        <div class="sector_segment d-flex align-items-center justify-content-between">
           <h6 data-toggle="collapse" href="#segment-{{segmentid}}">{{title}}</h6>
 
            <div class="d-flex action_btns">
                
                <!-- <a class="theme_text_color1 mr-3" href="">View</a> -->
                <a class="theme_text_color1"  data-action="createsegment" data-id={{segmentid}} title="{{#str}} edit, local_sector {{/str}}">
                <span class="bg-success text-white">
                <i class="fa fa-pencil" aria-hidden="true"></i></span></a>

                <a class="theme_text_color1 " data-action="deletesegment" data-id={{segmentid}}  title="{{#str}}delete,local_sector{{/str}}">
                <span class="bg-danger text-white">
                <i class="fa fa-trash-o" aria-hidden="true"></i>
                </span></a>

            </div>
        </div>
         <div class="collapse" id="segment-{{segmentid}}">
            {{#jobfamily}}
            <div class="sector_job ">
                <div class="job_header d-flex align-items-center justify-content-between">
                    <h6>({{#shared}}{{sectorcode}}{{/shared}}{{code}}) {{familyname}} {{#shared}}<sub>{{#str}} commontext,local_sector  {{/str}}</sub>{{/shared}}</h6>

                    <div class="d-flex action_btns">
                        <a href="{{config.wwroot}}./jobfamily.php?jobid={{jobfamilyid}}" class="theme_text_color1 mr-3" >{{# str}}view, local_sector{{/ str}}</a>
                        <a class="theme_text_color1" data-action="createjobfamily" data-id={{jobfamilyid}} data-segmentid = {{segmentid}} title="{{#str}} edit, local_sector {{/str}}" {{#shared}} data-shared =1 {{/shared}}><span class="bg-success text-white">
                <i class="fa fa-pencil" aria-hidden="true"></i></span></a>
                <a class="theme_text_color1 " data-action="deletejobfamily" data-id={{jobfamilyid}}  title="{{#str}}delete,local_sector{{/str}}"><span class="bg-danger text-white">
                <i class="fa fa-trash-o" aria-hidden="true"></i>
                </span></a>
                 
                    </div>
                </div>
            </div>
            {{/jobfamily}}
        </div>
    </div>
    {{/segment}}
</div>
{{/sector}}
{{/records}}
{{/response}}
