{{#response}}
{{#records}}
 <div class="row">
        <div class="col-md-12">
            <div class="tab-pane show active" id="tab_content_tab1" role="tabpanel"   aria-labelledby="tab1-tab">
                <div class="table-responsive mt-3">
                    <table class="table jobrole_table">
                        <thead> 
                            <tr>
                                <th scope="col">{{#str}}rolename , local_sector{{/str}}</th>
                                <th scope="col">{{#str}}job_code , local_sector{{/str}}</th>
                                <th scope="col">{{#str}}created_date , local_sector{{/str}}</th>
                                <th scope="col">{{#str}}user_created , local_sector{{/str}}</th>
                                <th class="text-center">{{#str}}actions , local_sector{{/str}}</th>
                            </tr>
                        </thead>
                       
                        <tbody>
                        {{# hascourses}}
                            <tr class="course_details_row">
                                
                                 <td>{{title}}</td>
                                 <td>{{code}}</td>
                                 <td>{{timemodified}}</td>
                                 <td>{{usermodified}}</td>
                                <td>
                                    <div class="d-flex action_btns justify-content-center"> 
                                       <div class="d-flex action_btns"> 
                                            <a href="#"  data-action="createjobrole_level" data-toggle ="tooltip" title="{{#str}}edit_role , local_sector{{/str}}" data-id={{id}} data-jobfamilyid={{jobfamily}}>
                                            <span class="bg-info text-white"><i class="fa fa-pencil fa-fw"></i></span></a>
                                            <a href="#" data-action="deletejobrole_level" data-toggle ="tooltip" title="{{#str}}delete_role , local_sector{{/str}}"  data-id={{id}} data-jobfamilyid={{jobfamily}} data-name="{{title}}"><span class="bg-danger text-white"><i class="fa fa-trash-o" aria-hidden="true"></i></span></a>
                                        </div>   
                                   </div>
                               </td>
                              
                            </tr>
                            <tr class="course_content_row">
                                <td colspan="5">
                                    <div>
                                        <div class="actions my-2">

                                            <a class="btn btn-outline-primary theme_text_color btn-sm" data-roleid ={{id}} data-jobfamilyid={{jobfamily}} data-action="createjobrole_resp">{{#str}}addresponsibility, local_sector{{/str}}</a>
                
                                             <a class="btn btn-outline-primary theme_text_color btn-sm" data-action="responsibilityview" data-roleid={{id}} data-jobfamilyid={{jobfamily}}>{{#str}} viewresponsibility, local_sector {{/str}}</a>

                                        </div>
                            
                                    </div>
                                </td>
                            </tr>
                            {{/ hascourses}}
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
{{#js}}
    require(['local_sector/jobrole_level'], function(s) {
        s.init();
    });
{{/js}}
 {{/ records}}
{{/ response}}



