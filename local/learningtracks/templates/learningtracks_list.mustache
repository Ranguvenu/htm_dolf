<div class="learningtracks_container">
		<!--<div class="d-flex mb-3 heading_div">
			<div class="d-inline-block theme-border-bottom-3">
				<h6 class="theme_text_color">{{#str}}available_learning_tracks,local_learningtracks{{/str}}</h6>
			</div>
		</div>-->

  {{# response}}
    <!---table start-->
    <div class="row">
        <div class="col-md-12">
            <div class="tab-pane show active" id="tab_content_tab1" role="tabpanel"   aria-labelledby="tab1-tab">
                <div class="table-responsive mt-3">
                    <table class="table training_prgrams_table">
                        <thead> 
                            <tr>
                                <th>{{#str}}learning_track,local_learningtracks{{/str}}</th>
                                <th>{{#str}}learning_items,local_learningtracks{{/str}}</th>
                                <th>{{#str}}status,local_learningtracks{{/str}}</th>
                                <th>{{#str}}enrolled,local_learningtracks{{/str}}</th>
                                <th>{{#str}}completed,local_learningtracks{{/str}}</th>
                                <th class="text-center">{{#str}}actions, local_trainingprogram{{/str}}</th>
                            </tr>
                        </thead>
                       
                        <tbody>
                            {{# records}}
                                <tr class="course_details_row">
                                    <td><a href="{{{viewurl}}}">{{name}}</a></td>
                                    <td>{{nolearningitems}}</td>
                                    <td>{{status}}</td>
                                    <td>{{enrollcount}}</td>
                                    <td>{{completed_count}}</td>
                                    <td>
                                        <div class="d-flex action_btns justify-content-center"> 
                                            <a href="{{{viewurl}}}" title="{{#str}}viewitems, local_learningtracks{{/str}}"><span class="bg-info text-white"><i class="fa fa-search-plus"></i></span></a>
                                            {{#edit}}
                                                <a href="#"  title = "{{#str}}edit{{/str}}" data-action="edittracker" data-id={{id}}>
                                                <span class="bg-info text-white"><i class="fa fa-pencil fa-fw"></i></span></a>
                                            {{/edit}}
                                            {{#delete}}
                                                <a href="#" title = "{{#str}}delete{{/str}}" data-action="deletetrack" data-id="{{id}}" data-name="{{name}}" data-usercount="{{enrollcount}}"><span class="bg-danger text-white"><i class="fa fa-trash-o" aria-hidden="true"></i></span></a>
                                            {{/delete}}
                                        </div>
                                    </td>
                                </tr>
                                <tr class="course_content_row">
                                    <td colspan="6">
                                        <div class="actions my-4">
                                            <!--<button class="btn btn-outline-primary theme_text_color"><a href="{{viewurl}}">{{#str}}viewitems, local_learningtracks{{/str}}</a></button>-->
                                            {{#action}}
                                            <button class="btn btn-outline-primary theme_text_color"  data-action="competencylist" data-id="{{id}}" data-addaction={{action}}>{{#str}}competency, local_learningtracks{{/str}}</button>
                                             {{/action}}
                                        </div>   
                                    </td>   
                                </tr>   
                            {{/records}}
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>    
    <!---table end-->
  {{/ response}}





























</div>