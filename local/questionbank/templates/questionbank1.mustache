<div class="row profileeditor">
    <div class="col align-self-end">
        <a href="{{config.wwroot}}./questionbank.php?form_status=0&st=new" target="_blank" tabindex="0" role="button" class="btn btn-secondary float-right mr-2">{{#str}}createquestionbank, local_questionbank{{/str}}</a>
    </div>
</div>
<div id="questionbank_container">
    <div class="row">
        <div class="col-12">
            <ul class="nav nav-tabs" id="question_bank_tabs" role="tablist">
                <li class="nav-item">
                    <a class="nav-link active" id="scheduled-tab" data-toggle="tab" href="#scheduled" role="tab" aria-controls="scheduled" aria-selected="true">Scheduled</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" id="completed-tab" data-toggle="tab" href="#completed" role="tab" aria-controls="completed" aria-selected="false">Completed</a>
                </li>
            </ul>
            <div class="tab-content" id="question_bank_tabsContent">
                <div class="tab-pane fade show active" id="scheduled" role="tabpanel" aria-labelledby="scheduled-tab">
                    <div id="questionbanks_wrapper">
                        <!-- questibank content -->
                        <div class="questionbank_content">
                            <div class="title_container d-flex align-items-center justify-content-between">
                               <h6 class="qbtitle">1. QB Workshop Name : Finance 101</h6>
                               <div>
                                    <h6><strong>Status Schedule</strong> <button class="btn btn-sm btn-primary ml-3 px-3">Edit</button></h6>
                               </div>
                            </div>
                            <div class="qb_details_div">
                                <div class="row">
                                    <div class="col-4 col-md-2">
                                        <h6>{{#str}}questiontarget, local_questionbank{{/str}}</h6>
                                        <h5><strong>50</strong></h5>
                                    </div>
                                    <div class="col-4 col-md-2">
                                        <h6>{{#str}}start_date, local_questionbank{{/str}}</h6>
                                        <h5>2/02/2022</h5>
                                    </div>
                                    <div class="col-4 col-md-2">
                                        <h6>{{#str}}start_time, local_questionbank{{/str}}</h6>
                                        <h5>9:30 AM to 05:20 PM</h5>
                                    </div>
                                    <div class="col-4 col-md-2">
                                        <h6>{{#str}}location, local_questionbank{{/str}}</h6>
                                        <h5>Riyadh</h5>
                                    </div>
                                    <div class="col-4 col-md-2">
                                        <h6>{{#str}}hall, local_questionbank{{/str}}</h6>
                                        <h5>Central Park</h5>
                                    </div>
                                    <div class="col-4 col-md-2">
                                        <h6>{{#str}}qb_admin, local_questionbank{{/str}}</h6>
                                        <h5>Karim</h5>
                                    </div>

                                  
                                    
                                </div>
                            </div>
                            <div class="qb_action_container mt-3 mb-4">
                                <div class="row">
                                    <div class="col-md-6">
                                        <button class="btn btn-outline-primary theme_text_color btn sm">{{#str}}competencies, local_questionbank{{/str}}</button>
                                        <button class="btn btn-outline-primary theme_text_color btn sm">{{#str}}topics, local_questionbank{{/str}}</button>
                                        <button class="btn btn-outline-primary theme_text_color btn sm">{{#str}}experts, local_questionbank{{/str}}</button>
                                        <button class="btn btn-outline-primary theme_text_color btn sm">{{#str}}view_details, local_questionbank{{/str}}</button>
                                        <button class="btn btn-outline-primary theme_text_color btn sm">{{#str}}view_questions, local_questionbank{{/str}}</button>
                                    </div>
                                    <div class="col-md-6 d-flex align-items-center justify-content-between">
                                        <span>{{#str}}status, local_questionbank{{/str}}: <strong>{{#str}}draft, local_questionbank{{/str}}</strong></span>
                                        <a href="">{{#str}}set_reminder, local_questionbank{{/str}}</a>
                                    </div>
                                </div>
                            </div>
                        </div>

                        



                    </div>
                </div>
                <div class="tab-pane fade" id="completed" role="tabpanel" aria-labelledby="completed-tab">
                
                </div>
            </div>
        </div>
