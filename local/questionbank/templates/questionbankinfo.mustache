
                       
                    {{#response}}
                    {{#records}}
                    {{# hascourses}}
                        <!-- questibank content -->
                        <div class="questionbank_content">
                            <div class="title_container d-flex align-items-center justify-content-between">
                               <h6 class="qbtitle"> {{#str}}qbworkshopname, local_questionbank{{/str}} : {{workshopname}}</h6>
                               <div>
                                    <h6>
                                      <!-- <strong>{{#str}}status_schedule, local_questionbank{{/str}}</strong> -->
                                      {{# actions}}
                                        <!-- <button class="btn btn-sm btn-primary ml-3 px-3">Edit</button> -->
                                        <a href="{{{questionbank_edit_url}}}/local/questionbank/questionbank.php?edit={{id}}&form_status=0&formid={{id}}"  tabindex="0" role="button" class="btn btn-sm btn-primary ml-3 px-3">{{#str}}edit, local_questionbank{{/str}}</a>
                                        <a href="#" data-action="deletequestionbank" data-id={{id}} data-name='{{workshopname}}' class="btn btn-sm btn-danger ml-3 px-3">{{#str}}delete, local_questionbank{{/str}}</a>
                                        {{/ actions}}
                                    </h6>
                               </div>
                            </div>
                            <div class="qb_details_div">
                                <div class="row">
                                    <div class="col-4 col-md-2">
                                        <h6>{{#str}}questiontarget, local_questionbank{{/str}}</h6>
                                        <h5><strong>{{noofquestions}}</strong></h5>
                                    </div>
                                    <div class="col-4 col-md-2">
                                        <h6>{{#str}}workshop_date, local_questionbank{{/str}}</h6>
                                        <h5>{{workshopdate}}</h5>
                                    </div>
                                    <div class="col-4 col-md-2">
                                        <h6>{{#str}}start_time, local_questionbank{{/str}}</h6>
                                        <h5>{{workshopstarttime}}</h5>
                                    </div>
                                    <div class="col-4 col-md-2">
                                        <h6>{{#str}}location, local_questionbank{{/str}}</h6>

                                        {{# locationstatus}}
                                            <a href="{{location}}"><i class="fa fa-map-marker"></i></a> 
                                        {{/ locationstatus}}
                                        {{^ locationstatus}}
                                            <h5>{{location}}</h5>
                                        {{/ locationstatus}}

                                    </div>
                                    <div class="col-4 col-md-2">
                                        <h6>{{#str}}hall, local_questionbank{{/str}}</h6>
                                        <h5>{{halladdress}}</h5>
                                    </div>
                                    <div class="col-4 col-md-2">
                                        <h6>{{#str}}qb_admin, local_questionbank{{/str}}</h6>
                                        <h5>{{workshopadmin}}</h5>
                                    </div>
                                    <div class="col-4 col-md-2">
                                        <h6>{{#str}}generatedcode, local_questionbank{{/str}}</h6>
                                        <h5>{{generatecode}}</h5>
                                    </div>
                                    
                                </div>
                            </div>
                            <div class="qb_action_container mt-3 mb-4">
                                <div class="row">
                                    <div class="col-md-6">
                                        <!-- <button class="btn btn-outline-primary theme_text_color btn sm">competencies</button> -->
                                          {{# actions}}
                                             <a data-action="viewcompetencies" data-id={{id}} data-name='{{workshopname}}'><button class="btn btn-outline-primary theme_text_color btn ">{{#str}}competencies, local_questionbank{{/str}}</button></a>
                                        {{# coursescount}}
                                            <a href="#" data-action="viewquestiontopics" data-id={{id}} data-name='{{workshopname}}'><button class="btn btn-outline-primary theme_text_color btn ">{{#str}}topics, local_questionbank{{/str}}</button></a>
                                         {{/coursescount}}
                                        <a data-action="viewexperts" data-id={{id}} data-name='{{workshopname}}' data-qbstatus='{{qbstatus}}' data-seats='{{availableseats}}'><button class="btn btn-outline-primary theme_text_color btn ">{{#str}}expert, local_questionbank{{/str}}</button></a>
                                       
<!--                                         <button class="btn btn-outline-primary theme_text_color btn sm">Experts</button>
 -->                                        <a href="{{{questionbank_workshop_url}}}"><button class="btn btn-outline-primary theme_text_color btn sm">{{#str}}viewdetails, local_questionbank{{/str}}</button></a>
                                          {{/actions}}
                                          {{# qcategoryid}}
                                           {{^ wcompleted}}
                                              {{# display_questions}}
                                               <a href="{{{questionbank_edit_url}}}/local/questionbank/questionbank_view.php?wid={{id}}&courseid=1&cat={{qcategory}}"><button class="btn btn-outline-primary theme_text_color btn sm">{{#str}}viewquestions, local_questionbank{{/str}}</button></a>
                                              {{/ display_questions}}
                                              {{^ display_questions}}
                                                   <span> {{{ontime}}}</span>
                                              {{/ display_questions}}
                                            {{/ wcompleted}} 
                                              {{/ qcategoryid}}
                                            {{# wcompleted}}
                                                        <a href="{{{questionbank_edit_url}}}/question/edit.php?courseid=1&qpage=0&category={{tocategory}}&qbshowtext=0&recurse=0&recurse=1&showhidden=0"><button class="btn btn-outline-primary theme_text_color btn sm">{{#str}}viewquestions, local_questionbank{{/str}}</button></a>
                                            {{/ wcompleted}}  
                                        
                                          {{# questionscount}}
                                          {{# actions}}
                                           <a href="#" data-action="movetoprodquestionbank" data-id={{id}} data-qcat={{qcategoryid}}><button class="btn btn-outline-primary theme_text_color btn ">{{#str}}movequestions, local_questionbank{{/str}}</button></a>   
                                           {{/actions}}
                                           {{/questionscount}}
                                         
                                    </div>
                                    <div class="col-md-6 d-flex align-items-center justify-content-between">
                                        <span>{{#str}}status, local_questionbank{{/str}}: <strong>{{workshopstatus}}</strong></span>
                                        <!-- <a href="">{{#str}}set_reminder, local_questionbank{{/str}}</a> -->
                                    </div>
         
                                </div>
       
                            </div>
                       
                        </div>
                {{/hascourses}}
                 {{/records}}
                 {{/response}}
               
                
                 
                <div class="tab-pane fade" id="completed" role="tabpanel" aria-labelledby="completed-tab">
                      <div class="completedquestionbank_content" id="completedquestionbank" data-region="completedquestionbank-preview-container">
                                <div data-region="completedquestionbank-count-container"></div>
                                <div data-region="completedquestionbank-list-container" id ="coursesid"></div> 
                        </div>
                </div>
    
