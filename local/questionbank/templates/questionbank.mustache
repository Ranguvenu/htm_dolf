<div id="questionbank_container" class="tabcontainer">
    <div class="row">
        <div class="col-12">
            <ul class="nav nav-tabs dolf-tabs" id="question_bank_tabs" role="tablist">
                <li class="nav-item">
                    <a class="nav-link active" data-action="contentTabs" id="contentTabs" data-toggle="tab" role="tab" data-controls=0 data-template={{template}} 
                    data-method={{action}} aria-selected="true">{{#str}}scheduled, local_questionbank{{/str}}</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" data-action="contentTabs" id="contentTabs" data-toggle="tab" role="tab" data-controls=1 data-template={{template}}
                    data-method={{action}}  aria-selected="false">{{#str}}completed, local_questionbank{{/str}}</a>
                </li>
            </ul>
            <div class="dolf_tabContent tab-content" id="question_bank_tabsContent">
                <div class="tab-pane fade show active" id="scheduled" role="tabpanel" aria-labelledby="scheduled-tab">
                    <div id="content_wrapper">
                        <div data-region='content_wrapper-list-container'>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

{{#js}}
    require(['theme_academy/DataFilter'], function(s) {
        s.init();
    });
{{/js}}
