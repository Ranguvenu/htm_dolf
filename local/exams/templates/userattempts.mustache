<div>
    <div class="reservationheadings pb-3">
    	<h4 class="text-themecolor">{{#str}}attemptscompleted, local_exams{{/str}}</h4>
    </div>
</div>
{{# attempts}}
	<div class="bg-white shadow rounded p-3 mt-2 available_times_container">
	    <div class="row mb-3">
	        <div class="col-md-12">
	            <div class="rounded bg-themecolor3 avail_dates_container p-3 position-relative">
	                <div class="d-flex flex-wrap align-items-center mb-3 justify-content-between">
	                    <div>
	                        <i class="fa fa-chevron-right text-themecolor2" aria-hidden="true"></i>
	                        {{#str}}attempt, local_exams{{/str}}:<strong> {{attempt}}</strong>
	                    </div>
	                    <div>
	                        <i class="fa fa-chevron-right text-themecolor2" aria-hidden="true"></i>
	                        {{#str}}profilecode, local_exams{{/str}}:<strong> {{profilecode}}</strong>
	                    </div>						
	                    <div>
	                        <i class="fa fa-chevron-right text-themecolor2" aria-hidden="true"></i>
	                        {{#str}}timestart, local_exams{{/str}}:<strong> {{timestart}}</strong>
	                    </div>
	                    <div>
	                        <i class="fa fa-chevron-right text-themecolor2" aria-hidden="true"></i>
	                        {{#str}}timefinish, local_exams{{/str}}:<strong> {{timefinish}}</strong>
	                    </div>
	                    <div>
	                        <i class="fa fa-chevron-right text-themecolor2" aria-hidden="true"></i>
	                        {{#str}}grade, local_exams{{/str}}:<strong> {{grade}}</strong>
	                    </div>
	                    <div>
	                        <i class="fa fa-chevron-right text-themecolor2" aria-hidden="true"></i>
	                        {{#str}}singlehall, local_hall{{/str}}:<strong> {{hallname}}</strong>
	                    </div>
	                </div>
	            </div>
	        </div>
	    </div>
	</div>
{{/ attempts}}
{{^ attempts}}
	{{#str}}nodataavailable, local_exams{{/str}}
{{/ attempts}}
