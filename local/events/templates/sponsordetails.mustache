<table class='generaltable' id="partnerlisttable">
    <tbody>
        <tr><td>{{#str}}sponsorname,local_events{{/str}}</td><td>{{sponsorname}}</td></tr>
        <tr><td>{{# str}}category, local_events{{/ str}}</td><td>{{category}}</td></tr>
        {{#showprice}}
            <tr><td>{{# str}}amtpay, local_events{{/ str}}</td><td>{{amount}}</td></tr>
        {{/showprice}}
        <tr><td>{{# str}}logo, local_events{{/ str}}</td><td> 
                <div class="partnerlogo">
                {{#logo}}
                    <img src="{{logo}}"></div>
                {{/logo}}
                {{^logo}}--{{/logo}}
            </td>
        </tr>
    </tbody>
</table>