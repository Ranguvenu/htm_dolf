{{#response}}
    {{#records}}
    <div class= "table-responsive">
        <table class="table">
            <thead class="thead-light">
                <tr>
                  <th scope="col">{{# str}} refund_type, local_trainingprogram{{/ str}}</th>
                  <th scope="col">{{# str}} refund_entitytype, local_trainingprogram{{/ str}}</th>
                  <th scope="col">{{# str}} ownedby, local_trainingprogram{{/ str}}</th>
                  <th scope="col">{{# str}} attempt, local_trainingprogram{{/ str}}</th>
                  <th scope="col">{{# str}} refund_dayfrom, local_trainingprogram{{/ str}}</th>
                  <th scope="col">{{# str}} refund_dayto, local_trainingprogram{{/ str}}</th>
                  <th scope="col">{{# str}} refund_dedtype, local_trainingprogram{{/ str}}</th>
                  <th scope="col">{{# str}} refund_dedvalue, local_trainingprogram{{/ str}}</th>
                  <th class="text-center">{{# str}} actions, local_userapproval{{/ str}}</th>
                </tr>
            </thead>
          <tbody>
          {{# hascourses}}
              <tr>
                    <td>{{{type}}}</a></td>
                    <td>{{entitytype}}</td>
                    <td>{{ownedby}}</td>
                    <td>{{moreattempts}}</td>
                    <td>{{dayfrom}}</td>                                                                     
                    <td>{{dayto}}</td>                                                 
                    <td>{{dedtype}}</td>                                                 
                    <td>{{dedvalue}}</td>  
                    <td>
                      <div class="d-flex action_btns justify-content-center">
                         <a href="#"  data-action="createrefundsetting" data-id={{id}} data-toggle ="tooltip" title={{#str}}edit,local_trainingprogram{{/str}}><span class="bg-info text-white"><i class="fa fa-pencil"></i></span></a>
                    
                        <a href="#" data-action="deleterefundsetting" data-id={{id}}  data-toggle ="tooltip" title={{#str}}delete,local_trainingprogram{{/str}}><span class="bg-danger text-white"><i class="fa fa-trash-o" aria-hidden="true"></i></span></a>
                      </div>
                  </td>      
              </tr>
           {{/ hascourses}} 
          </tbody>
        </table>
        </div>
    {{/ records}}
{{/ response}}

{{#js}}
    require(['local_trainingprogram/refundsetting'], function(s) {
        s.init();
    });
{{/js}}
